git init
rm -rf .git
rm <file_name>
git add -A 				#adds all the files to the staging area
git reset <file_name> 	#removes from the staging area
git reset 				#removes all
git status
git log
git branch <name>		#creates a branch
git checkout <br_name>	#switches to that branch

git checkout main
git merge <br_name>
git remote
git remote add origin <url>
git remote
git push -u origin master


touch .gitignore		#add log.txt to gitignore
touch log.txt			#/dir add directories to be ignored
	


esc :wq
